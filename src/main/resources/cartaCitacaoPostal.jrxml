				<text><![CDATA[CARTA DE CITAÇÃO POSTAL]]></text>
			</staticText>
			<staticText>
				<reportElement positionType="Float" x="10" y="210" width="535" height="85" uuid="8b4f69e2-1e54-4dd0-9313-e01e55790a8f">
					<property name="local_mesure_unitheight" value="pixel"/>
