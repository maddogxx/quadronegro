					<property name="com.jaspersoft.studio.unit.height" value="px"/>
				</reportElement>
				<textElement textAlignment="Justified" verticalAlignment="Top">
					<font size="9"/>
				</textElement>
				<textFieldExpression><![CDATA["Valor do débito atualizado em " + $P{DATA_DEBITO_ATUALIZADO} + "."]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement positionType="Float" x="10" y="320" width="535" height="15" uuid="af8b254d-8041-4ee2-bc5b-ee90c339af50">
					<property name="local_mesure_unitheight" value="pixel"/>
